{
  "name": "kanv19/dbal-clickhouse",
  "type": "library",
  "version": "1.2.0",
  "description": "Doctrine DBAL driver for ClickHouse",
  "keywords": [
    "doctrine",
    "dbal",
    "clickhouse",
    "driver"
  ],
  "homepage": "http://github.com/kanv19/dbal-clickhouse",
  "license": "MIT",
  "authors": [
    {
      "name": "Mochalygin Aleksandr",
      "email": "a@mochalygin.ru"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "smi2/phpClickHouse": "^1.2",
    "doctrine/dbal": ">=2.6"
  },
  "autoload": {
    "psr-4": {
      "FOD\\DBALClickHouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FOD\\DBALClickHouse\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  }
}
